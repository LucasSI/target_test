2)
a = 0
b = 1
fib = []
fib.append(a)
fib.append(b)
for i in range(2,100):
    c = a+b
    fib.append(c)
    a = b
    b = c
numero = int(input("digite um numero:"))
for i in range(100):
    if numero == fib[i]:
        print("O numero pertence a sequência")
        break
